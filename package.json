{
	"name": "osmhu-website",
	"version": "0.0.0",
	"description": "OpenStreetMap Hungary",
	"private": true,
	"browser": "js/frontpage.js",
	"dependencies": {
		"autocomplete.js": "^0.38.0",
		"browser-update": "^3.3.33",
		"clipboard": "^2.0.8",
		"coordinate-parser": "^1.0.7",
		"core-js": "^3.17.3",
		"date-fns": "^2.23.0",
		"fg-loadcss": "^3.1.0",
		"jquery": "^3.6.0",
		"js-cookie": "^3.0.1",
		"leaflet": "1.8.0",
		"leaflet-overpass-layer": "^2.8.4",
		"leaflet-providers": "^1.13.0",
		"leaflet.locatecontrol": "^0.76.1",
		"leaflet.markercluster": "^1.5.3",
		"loglevel": "^1.7.1",
		"normalize.css": "^8.0.1",
		"opening_hours": "^3.7.0",
		"present": "^1.0.0",
		"query-string": "^7.0.1",
		"regenerator-runtime": "^0.13.9",
		"svgxuse": "^1.2.6"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.6",
		"babel-jest": "^29.2.2",
		"babel-loader": "^8.2.2",
		"chromedriver": "^101.0.0",
		"editorconfig-checker": "^4.0.2",
		"es6-plato": "^1.2.3",
		"eslint": "^8.25.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.3",
		"geckodriver": "^3.0.1",
		"gulp": "^4.0.2",
		"gulp-clean-css": "^4.3.0",
		"gulp-concat": "^2.6.1",
		"gulp-minify": "^3.1.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-strip-css-comments": "^2.0.0",
		"jest": "^29.2.2",
		"jest-environment-jsdom": "^29.2.2",
		"jest-serializer-html": "^7.1.0",
		"jshint": "^2.13.1",
		"package-changed": "^1.7.0",
		"selenium-webdriver": "^4.1.2",
		"stylelint": "^14.2.0",
		"stylelint-config-twbs-bootstrap": "^3.0.1",
		"terser-webpack-plugin": "^5.2.4",
		"uglifyify": "^5.0.2",
		"webpack": "^5.53.0",
		"webpack-cli": "^4.8.0",
		"worker-loader": "^3.0.8"
	},
	"scripts": {
		"analysis": "es6-plato -r -d report js",
		"lint:css": "stylelint \"css/{common,info}/*.css\"",
		"lint:editorconfig": "editorconfig-checker",
		"lint:js": "eslint js/ && jshint js/",
		"install-if-changed": "package-changed --hash-filename .tmp/.package-json-hash.txt",
		"build": "webpack --config js/webpack.config.js --env production",
		"build-css": "gulp --gulpfile js/gulpfile.js",
		"build-development": "webpack --config js/webpack.config.js --env development",
		"test": "jest --coverage --roots js/",
		"test-watch": "jest --watchAll --coverage --roots js/",
		"test-e2e": "jest --config test-e2e/jest.config.js"
	},
	"engines": {
		"node": ">=14.15.3"
	},
	"jest": {
		"snapshotSerializers": [
			"jest-serializer-html"
		],
		"moduleFileExtensions": [
			"js",
			"json"
		],
		"transform": {
			"^.+\\.js$": "babel-jest"
		},
		"testEnvironment": "jsdom"
	},
	"license": "MIT"
}
